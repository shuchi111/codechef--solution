#include <iostream>
using namespace std;
void check(int x,int y)
{
    if(x>y) cout<<"NO"<<endl;
    else if(y>x+200) cout<<"NO"<<endl;
    else cout<<"YES"<<endl;
}
int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b;
	    cin>>a>>b;
	    check(a,b);
	}
	
	return 0;
}
